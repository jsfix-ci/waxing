{
  "name": "waxing",
  "version": "0.2.0",
  "description": "JS package to decrypt password-protected Office documents",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/*",
    "!dist/*.spec.js",
    "src/*",
    "!src/*.spec.js"
  ],
  "scripts": {
    "lint": "standard",
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --copy-files",
    "pretest": "npm run lint && npm run build",
    "test": "mocha './dist/test.spec.js'",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seald/waxing.git"
  },
  "author": "Seald",
  "license": "MIT",
  "dependencies": {
    "event-stream": "^4.0.1",
    "file-type": "^12.4.0",
    "node-forge": "^0.9.1",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "centimaitre": "^1.0.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "fs-jetpack": "^2.2.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "standard": "^14.3.1"
  }
}
